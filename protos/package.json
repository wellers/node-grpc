{
	"name": "helloworld-protos",
	"scripts": {
		"build"	: "npx protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=dist --ts_proto_opt=outputServices=grpc-js --ts_proto_opt=esModuleInterop=true -I=src/ src/helloworld.proto"
	},
	"dependencies": {
		"@grpc/grpc-js": "1.8.14",
		"@tsconfig/node18": "1.0.1",
		"protoc": "1.1.3",		
		"ts-proto": "1.147.2",
		"typescript": "4.8.3"
	}
}
